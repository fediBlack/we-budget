{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@webudget/shared-types$": "<rootDir>/../../../packages/shared-types/dist",
    "^@webudget/shared-types/(.*)$": "<rootDir>/../../../packages/shared-types/dist/$1"
  }
}
